{
  "version": 2,
  "name": "Christopher Ksiazak Developer Page",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    { "src": "/", "dest": "index.html" },
    { "src": "/(.*)", "dest": "404.html" }
  ]
}
